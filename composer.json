{
    "name": "crishellco/laravel-console-dusk",
    "description": "Laravel Console Dusk allows the usage of Laravel Dusk in Laravel/Laravel Zero artisan commands.",
    "keywords": ["console", "command-line", "php", "cli", "laravel-zero", "laravel", "Laravel Dusk", "artisan", "symfony"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/crishellco/laravel-console-dusk/issues",
        "source": "https://github.com/crishellco/laravel-console-dusk"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/crishellco/dusk"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "crishellco/dusk": "^3.0|^4.0",
        "illuminate/console": "5.6.*|5.7.*",
        "illuminate/support": "5.6.*|5.7.*",
        "crishellco/laravel-console-task": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "crishellco\\LaravelConsoleDusk\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NunoMaduro\\LaravelConsoleDusk\\LaravelConsoleDuskServiceProvider"
            ]
        }
    },
    "suggest": {
        "phpunit/phpunit": "Required to perform browser test assertions."
    }
}
